{
	"compilerOptions": {
		"target": "ES2022",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"strict": true,
		"lib": [
			"ES2022"
		],
		"types": [
			"Node"
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"outDir": "../../dist/service",
		"rootDir": "./src",
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}